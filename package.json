{
  "name": "javascript",
  "version": "0.1.0",
  "description": "Javascript examples",
  "keywords": [
    "javascript",
    "js",
    "typescript",
    "ts"
  ],
  "homepage": "https://github.com/krbaio3/javascript#readme",
  "bugs": {
    "url": "https://github.com/krbaio3/javascript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krbaio3/javascript.git"
  },
  "license": "MIT",
  "author": "krbaio3",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && parcel ./src/index.html",
    "build:prod": "rimraf dist && parcel build ./src/index.html",
    "commit": "cz",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "lint:fix": "eslint -c .eslintrc.js --ext .ts ./src --fix",
    "prettier": "prettier --write .",
    "release": "standard-version",
    "start": "run-p -l type-check:watch && start:dev",
    "start:dev": "rimraf dist && parcel ./src/index.html --open",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start:prod": "run-p -l build:prod && http-server dist"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/parser": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/types": "^7.11.5",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@types/jest": "^26.0.14",
    "@types/node": "~12.12.38",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "http-server": "^0.12.3",
    "babel-jest": "^26.5.2",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "typescript": "^3.9.5",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "rimraf": "~3.0.2",
    "standard-version": "^9.0.0"
  },
  "peerDependencies": {}
}
