{
 "compilerOptions": {
  "lib": ["DOM", "DOM.Iterable", "ESNext"],
  "target": "ES6",
  "module": "ES2020",
  "allowJs": true,
  "outDir": "dist",
  "moduleResolution": "node",
  "declaration": false,
  "noImplicitAny": false,
  "allowSyntheticDefaultImports": true,
  "sourceMap": true,
  "noLib": false,
  "suppressImplicitAnyIndexErrors": true,
  "skipLibCheck": true,
  "esModuleInterop": true,
  "baseUrl": "./src/"
 },
 "compileOnSave": true,
 "exclude": ["node_modules"],
 "include": ["src"]
}
