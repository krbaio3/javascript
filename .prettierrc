{
  "$schema": "http: //json.schemastore.org/prettierrc",
  "singleQuote": true,
  "trailingComma": "es6",
  "semi": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "proseWrap": "always",
  "arrowParens": "always",
  "htmlWhitespaceSensitivity": "strict",
  "insertPragma": false,
  "requirePragma": false
}
